{
    "compilerOptions": {
        "target": "ES6",
        "lib": ["ES6"],
        "module": "commonjs",
        "types": ["node", "aws-sdk"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "rootDir": "./",
        "outDir": ".build",
        "sourceMap": true,
        "noImplicitAny": true
    }
}
